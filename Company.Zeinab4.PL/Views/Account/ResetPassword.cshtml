@model ResetPasswordDTO
@{
    ViewData["Title"] = "ResetPassword";
    Layout = "~/Views/Shared/_AuthLayout.cshtml";
}


<div class="main-agileinfo">
	<div class="agileits-top">
		<div asp-validation-summary="All" class="alert alert-danger"></div>
		<form asp-action="ResetPassword">

	
			<input class="text" type="password" asp-for="NewPassword" placeholder="New Password" required="">
			<input class="text w3lpass" type="password" asp-for=" ConfirmPassword" placeholder="Confirm New Password" required="">
			<div class="wthree-text">
			
				<div class="clear"></div>
			</div>
			<input type="submit" value="ResetPassword">
		</form>
		
	</div>
</div>
@section SectionVailidation {
	<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
	<script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>

}



